<?xml version="1.0" encoding="ASCII"?>
<SmartHouse:House
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:SmartHouse="http://dsg.cs.tcd.ie/smarthouse"
    xsi:schemaLocation="http://dsg.cs.tcd.ie/smarthouse SmartHouse.ecore"
    name="myhouse"
    eprice="25.0"
    time="10"
    outtemp="5">
  <room temp="9.0"
      bright="4000.0"
      name="liv">
    <window
        name="w1"/>
    <window
        name="w2"/>
    <heating
        name="h1"/>
    <heating
        name="h2"
        level="2"/>
    <light level="3"/>
    <light level="3"/>
    <ac level="0"/>
    <projector
        brightness="5"
        volume="26"/>
  </room>
  <room bright="30.0"
      air="6"
      name="kitchen">
    <cooker
        on="true"/>
  </room>
  <room temp="15.0"
      bright="1000"
      air="6"
      name="bed1">
    <window
        name="w1"
        curtainOn="true"/>
    <heating
        name="h"
        level="2"/>
  </room>
  <room temp="10.0"
      bright="2000"
      name="bed2">
    <window
        name="w1"
        opened="true"/>
    <light level="0"/>
  </room>
  <wh temp="20"
      on="true"/>
  <ev name="donkey"
      pluged="true"
      level="10"/>
</SmartHouse:House>
